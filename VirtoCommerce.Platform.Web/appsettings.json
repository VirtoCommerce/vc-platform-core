{
    "ConnectionStrings": {
        "VirtoCommerce": "(local);Initial Catalog=VirtoCommerce3.0;Persist Security Info=True;User ID=virto;Password=virto;MultipleActiveResultSets=True;Connect Timeout=30"
    },
    "Logging": {
        "IncludeScopes": true,
        "LogLevel": {
            "Default": "Trace"
        }
    },
    "smidge": {
        "dataFolder": "App_Data/Smidge",
        "version": "1"
    },

  "VirtoCommerce": {
    "ActivationUrl": "https://virtocommerce.com/admin/api/licenses/activate/",
    "LicenseFilePath": "App_Data\\VirtoCommerce.lic",
    "LicensePublicKeyPath": "App_Data\\public.key",
    "UploadUrl": "App_Data\\Uploads"
  },
    "Assets": {
        "Provider": "FileSystem",
        "FileSystem": {
            "rootPath": "App_Data\\Assets",
            "publicUrl": "http://localhost/admin/assets"
        },
        "AzureBlobStorage": {
            "ConnectionString": "",
            "CdnUrl": ""
        }

    },
    "Notifications": {
        "Gateway": "Smtp",
        "DefaultSender": "noreply@gmail.com",
        "Smtp": {
            "SmtpServer": "",
            "Port": 587,
            "Login": "",
            "Password": ""

        },
        "SendGrid": {
            "ApiKey": ""
        }
    },
    "IdentityOptions": {
        "Password": {
            "RequiredLength": 8,
            "RequireDigit": false,
            "RequireNonAlphanumeric": false
        },
        "Lockout": {
            "DefaultLockoutTimeSpan": "0:15:0"
        }
    }
}
